<?xml version="1.0" encoding="utf-8"?><!-- 描述: -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >


    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="oval">
            <solid
                android:width="1dp"
                android:color="@color/color_blue" />
        </shape>
    </item>

    <item >
        <shape android:shape="oval" >
            <stroke
                android:width="1dp"
                android:color="@color/color_blue" />
        </shape>
    </item>



</layer-list>